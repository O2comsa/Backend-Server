{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "arcanedev/log-viewer": "^10.0",
        "askedio/laravel-soft-cascade": "^10.0",
        "consoletvs/charts": "^6.6",
        "doctrine/dbal": "^3.0",
        "intervention/image": "^2.7",
        "laravel-notification-channels/fcm": "^3.2",
        "laravel/framework": "^10.0",
        "laravel/legacy-factories": "^1.3",
        "laravel/passport": "^11.0",
        "laravel/slack-notification-channel": "^3.1",
        "laravel/socialite": "^5.10",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.2",
        "laravelcollective/html": "^6.4",
        "lcobucci/jwt": "^5.1",
        "maatwebsite/excel": "*",
        "proengsoft/laravel-jsvalidation": "^4.8",
        "santigarcor/laratrust": "^8.2",
        "spatie/browsershot": "^3.60",
        "symfony/http-client": "^6.3",
        "symfony/mailgun-mailer": "^6.3",
        "yajra/laravel-datatables": "^10.1",
        "yajra/laravel-datatables-buttons": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "beyondcode/laravel-dump-server": "^1.4",
        "spatie/laravel-ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    }
}
